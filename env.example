# Database
DATABASE_URL=postgresql://user:password@localhost:5432/dubai_real_estate

# OpenAI (pour agent CIO)
OPENAI_API_KEY=sk-...

# DLD API (Dubai Pulse - Dubai Land Department)
# Obtenir l'accès sur : https://www.dubaipulse.gov.ae
DLD_API_KEY=your_client_id_here
DLD_API_SECRET=your_client_secret_here
DLD_API_BASE_URL=https://api.dubaipulse.gov.ae

# Bayut API (lead indicators - annonces live)
# Obtenir l'accès : https://www.bayut.com/partnerships
BAYUT_API_KEY=your_bayut_api_key
BAYUT_API_URL=https://api.bayut.com/v1

# Makani Geocoding (Dubai Municipality - matching & localisation)
# Obtenir l'accès : https://geohub.dubaipulse.gov.ae
MAKANI_API_KEY=your_makani_api_key
MAKANI_API_URL=https://api.dubaipulse.gov.ae/makani

# DDA Planning & Zoning (signaux en avance)
# Obtenir l'accès : https://www.dm.gov.ae/open-data
DDA_API_KEY=your_dda_api_key
DDA_API_URL=https://api.dm.gov.ae/v1

# Listings API (legacy - remplacé par Bayut)
LISTINGS_API_KEY=your_listings_api_key
LISTINGS_API_URL=https://api.authorized-provider.com/v1

# Developers API (pipeline supply)
DEVELOPERS_API_KEY=your_developers_api_key

# Refresh intervals (minutes)
POLLING_INTERVAL_MINUTES=15
CACHE_TTL_MINUTES=10

# Alertes
ALERT_EMAIL=your-email@example.com
ALERT_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Timezone
TIMEZONE=Asia/Dubai

# Streamlit
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0
